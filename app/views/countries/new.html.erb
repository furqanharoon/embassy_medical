<div class="page-header">
	<h2>
		Add New Country 
	</h2>	
</div>
<div class="page-header">
<div class="row">
	<%=  form_for :Country ,:url=>{:action=>"create"} do |f| %>
	<div class="row">
		<div class="col-sm-2">Counter Name</div>
		<div class="col-sm-3"><%= f.text_field(:country_name) %></div>
	</div>
	<div class="row">
		<div class="col-sm-2">Enable</div>
		<div class="col-sm-3"><%= f.check_box(:status) %></div>
	</div>
	<div class="div">
		<div class="col-sm-2"></div>
		<div class="col-sm-3"><%= f.submit "Save" %></div>
	</div>
	<%end%>
</div>
</div>